<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    android:gravity="center_horizontal">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="61.5dp"
        android:layout_marginTop="30px"
        android:orientation="horizontal"
        android:gravity="center_horizontal">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="333.5dp"
            android:layout_height="61.5dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:hint="@string/android_ep"
            android:id="@+id/textInputEditTextentry" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="56.0dp"
        android:layout_marginTop="20px"
        android:orientation="horizontal"
        android:gravity="center_horizontal">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="333.5dp"
            android:layout_height="56.0dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:hint="@string/android_exp"
            android:id="@+id/textInputEditTextexit" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50.5dp"
        android:layout_marginTop="20px"
        android:orientation="horizontal"
        android:gravity="center_horizontal">
        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="333.5dp"
            android:layout_height="52.5dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:hint="@string/android_bid"
            android:id="@+id/textInputEditTextbid" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48.5dp"
        android:layout_marginTop="5px"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:id="@+id/relativeLayout1">
        <Button
            android:text="@string/android_calculate_income"
            android:layout_width="177.5dp"
            android:layout_height="51.5dp"
            android:backgroundTint="#8dc351"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/btncalcu"
            android:layout_marginBottom="6.0dp"
            android:fontFamily="monospace"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="395.5dp"
        android:layout_height="20.5dp"
        android:layout_marginTop="20px"
        android:gravity="center_horizontal">
        <TextView
            android:text="@string/android_strexpectedpnl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/PNLSTRING"
            android:textAlignment="center"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="57.5dp"
        android:layout_marginTop="5px"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:id="@+id/relativeLayout1">
        <Button
            android:text="@string/app_thumbnail_heading"
            android:layout_width="278.5dp"
            android:layout_height="71.0dp"
            android:backgroundTint="#8dc351"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/btnamount"
            android:layout_marginBottom="6.0dp"
            android:fontFamily="monospace"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="395.5dp"
        android:layout_height="20.5dp"
        android:layout_marginTop="30px"
        android:gravity="center_horizontal">
        <TextView
            android:text="@string/android_texback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/textViewback"
            android:textAlignment="center"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="4.5dp"
        android:layout_marginTop="30px"
        android:gravity="center_horizontal"
        android:background="#555">
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="395.5dp"
        android:layout_height="46.0dp"
        android:layout_marginTop="10px"
        android:gravity="center_horizontal">
        <TextView
            android:text="@string/android_des1"
            android:layout_width="313.5dp"
            android:layout_height="44.5dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/textViewdes1"
            android:textAlignment="center"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="395.5dp"
        android:layout_height="44.0dp"
        android:layout_marginTop="10px"
        android:gravity="center_horizontal">
        <TextView
            android:text="@string/android_des2"
            android:layout_width="313.5dp"
            android:layout_height="45.0dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/textViewdes2"
            android:textAlignment="center"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="395.5dp"
        android:layout_height="41.0dp"
        android:layout_marginTop="10px"
        android:gravity="center_horizontal">
        <TextView
            android:text="@string/android_des3"
            android:layout_width="313.5dp"
            android:layout_height="41.5dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/textViewdes3"
            android:textAlignment="center"/>
    </RelativeLayout>
</LinearLayout>
